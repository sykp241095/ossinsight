  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "NODE_ENV=development fastify start -p ${PIPELINE_SERVER_PORT:-30003} -P --ignore-watch=.ts$ -L dist/logger.js -l info -w dist/app.js",
